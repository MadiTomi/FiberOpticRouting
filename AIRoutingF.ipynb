        "    buildings_file = \"buildings.geojson\"\n",
        "\n",
        "    # –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–∞–ª–∏—á–∏–µ —Ñ–∞–π–ª–æ–≤\n",
        "    for file in [roads_file, muffs_file, manholes_file, buildings_file]:\n",
        "        if file and not os.path.exists(file):\n",
        "            print(f\"–§–∞–π–ª {file} –Ω–µ –Ω–∞–π–¥–µ–Ω, –±—É–¥—É—Ç –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω—ã —Ç–µ—Å—Ç–æ–≤—ã–µ –¥–∞–Ω–Ω—ã–µ.\")\n",
        "\n",
        "    # –°–æ–∑–¥–∞–µ–º –æ–±—ä–µ–∫—Ç —Å–µ—Ç–∏\n",
        "    network = FiberNetworkFromRealData(\n",
        "        roads_file=roads_file,\n",
        "        muffs_file=muffs_file,\n",
        "        manholes_file=manholes_file,\n",
        "        buildings_file=buildings_file\n",
        "    )\n",
        "\n",
        "    # –ü—Ä–æ–≤–µ—Ä—è–µ–º, —á—Ç–æ –¥–∞–Ω–Ω—ã–µ –∑–∞–≥—Ä—É–∂–µ–Ω—ã\n",
        "    print(f\"–î–æ—Ä–æ–≥–∏: {len(network.roads)}, –ú—É—Ñ—Ç—ã: {len(network.muffs)}, –ö–æ–ª–æ–¥—Ü—ã: {len(network.wells)}, –ó–¥–∞–Ω–∏—è: {len(network.buildings)}\")\n",
        "    print(f\"–¢–µ—Å—Ç–æ–≤—ã–µ –∑–¥–∞–Ω–∏—è: {len(network.test_buildings)}, –ê–¢–°: {network.ats}\")\n",
        "\n",
        "    # –í—ã–±–∏—Ä–∞–µ–º –∑–¥–∞–Ω–∏–µ —Å–ª—É—á–∞–π–Ω–æ (—Ä–µ–∞–ª—å–Ω–æ–µ –∏–ª–∏ —Ç–µ—Å—Ç–æ–≤–æ–µ)\n",
        "    if network.buildings:\n",
        "        target_building = random.choice(network.buildings)\n",
        "        building_point = target_building['centroid']\n",
        "        print(f\"–í—ã–±—Ä–∞–Ω–æ —Ä–µ–∞–ª—å–Ω–æ–µ –∑–¥–∞–Ω–∏–µ: {target_building['name']}, —Ü–µ–Ω—Ç—Ä–æ–∏–¥: {building_point}\")\n",
        "    elif network.test_buildings:\n",
        "        target_building = random.choice(network.test_buildings)\n",
        "        building_point = (target_building[0], target_building[1])\n",
        "        print(f\"–í—ã–±—Ä–∞–Ω–æ —Ç–µ—Å—Ç–æ–≤–æ–µ –∑–¥–∞–Ω–∏–µ, –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç—ã: {building_point}\")\n",
        "    else:\n",
        "        print(\"–ù–µ—Ç –¥–æ—Å—Ç—É–ø–Ω—ã—Ö –∑–¥–∞–Ω–∏–π –¥–ª—è –ø–æ–¥–∫–ª—é—á–µ–Ω–∏—è.\")\n",
        "        exit(1)\n",
        "\n",
        "    # –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–∞–ª–∏—á–∏–µ –º—É—Ñ—Ç\n",
        "    if not network.muffs:\n",
        "        print(\"–ù–µ—Ç –¥–æ—Å—Ç—É–ø–Ω—ã—Ö –º—É—Ñ—Ç –¥–ª—è –ø–æ–¥–∫–ª—é—á–µ–Ω–∏—è.\")\n",
        "        exit(1)\n",
        "\n",
        "    # –í—ã–±–∏—Ä–∞–µ–º –±–ª–∏–∂–∞–π—à—É—é –º—É—Ñ—Ç—É\n",
        "    nearest_muff, distance = network.find_nearest_point(building_point, network.muffs)\n",
        "    print(f\"–ë–ª–∏–∂–∞–π—à–∞—è –º—É—Ñ—Ç–∞: {nearest_muff}, —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ: {distance}\")\n",
        "\n",
        "    # –°—Ç—Ä–æ–∏–º –º–∞—Ä—à—Ä—É—Ç: –ê–¢–° ‚Üí –º—É—Ñ—Ç–∞ ‚Üí –∑–¥–∞–Ω–∏–µ\n",
        "    path1 = network.find_path_through_infrastructure(network.ats, (nearest_muff[0], nearest_muff[1]))\n",
        "    path2 = network.find_path_through_infrastructure((nearest_muff[0], nearest_muff[1]), building_point)\n",
        "    full_path = path1[:-1] + path2  # —Å–æ–µ–¥–∏–Ω—è–µ–º, —É–±–∏—Ä–∞—è –¥—É–±–ª–∏—Ä—É—é—â—É—é –º—É—Ñ—Ç—É\n",
        "    print(f\"Path1 (–ê–¢–° ‚Üí –º—É—Ñ—Ç–∞): {path1}\")\n",
        "    print(f\"Path2 (–º—É—Ñ—Ç–∞ ‚Üí –∑–¥–∞–Ω–∏–µ): {path2}\")\n",
        "    print(f\"Full path: {full_path}\")\n",
        "\n",
        "    # –í–∏–∑—É–∞–ª–∏–∑–∞—Ü–∏—è\n",
        "    try:\n",
        "        network.visualize_path_plotly(start=network.ats, end=building_point, path_coords=full_path)\n",
        "    except Exception as e:\n",
        "        print(f\"–û—à–∏–±–∫–∞ –ø—Ä–∏ –≤–∏–∑—É–∞–ª–∏–∑–∞—Ü–∏–∏: {e}\")\n",
        "        print(\"–ü—Ä–æ–≤–µ—Ä—å—Ç–µ 'network_visualization.html' –¥–ª—è —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤.\")"
      ]
